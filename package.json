{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "Start boilerplate for static markup",
  "main": "index.js",
  "scripts": {
    "gulp": "cross-env NODE_ENV=dev gulp",
    "build": "cross-env NODE_ENV=prod gulp build",
    "pugLint": "glob-exec \"./src/pages/**/*.pug\" -- \"pug-lint {{files.join(' ')}}\"",
    "styleLint": "stylelint \"./src/assets/styles/**/*.pcss\"",
    "styleLintFix": "stylelint \"./src/assets/styles/**/*.pcss\" --fix",
    "eslint": "eslint \"./src/assets/scripts/**/*.js\"",
    "eslintFix": "eslint \"./src/assets/scripts/**/*.js\" --fix",
    "test": "npm run pugLint && npm run styleLint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmValeriya/gulp-template.git"
  },
  "keywords": [
    "gulp",
    "pug",
    "postcss"
  ],
  "author": "Smirnova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SmValeriya/gulp-template/issues"
  },
  "homepage": "https://github.com/SmValeriya/gulp-template#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "glob-exec": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rm": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgo": "^2.2.1",
    "husky": "^7.0.0",
    "postcss": "^8.3.9",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-rgb": "^1.0.0",
    "precss": "^4.0.0",
    "prettier": "^2.4.1",
    "pug-lint": "^2.6.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.59.1",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "normalize.css": "^8.0.1"
  }
}
