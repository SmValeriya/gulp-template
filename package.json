{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "Start boilerplate for static markup",
  "main": "index.js",
  "scripts": {
    "gulp": "cross-env NODE_ENV=dev gulp",
    "build": "cross-env NODE_ENV=prod gulp build",
    "pugLint": "glob-exec \"./src/pages/**/*.pug\" -- \"pug-lint {{files.join(' ')}}\"",
    "styleLint": "stylelint \"./src/assets/styles/**/*.pcss\"",
    "styleLintFix": "stylelint \"./src/assets/styles/**/*.pcss\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmValeriya/gulp-template.git"
  },
  "keywords": [
    "gulp",
    "pug",
    "postcss"
  ],
  "author": "Smirnova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SmValeriya/gulp-template/issues"
  },
  "homepage": "https://github.com/SmValeriya/gulp-template#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "glob-exec": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "postcss": "^8.3.9",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-rgb": "^1.0.0",
    "precss": "^4.0.0",
    "pug-lint": "^2.6.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
